[Unit]
Description=Kubernetes proxy server
After=docker.service
Wants=docker.service

[Service]
ExecStart=/usr/local/bin/kube-proxy \
--master=lb \
--kubeconfig=/etc/kubernetes/workerworkerID/kubeconfig.yaml \
--logtostderr=true
Restart=on-failure
RestartSec=5

[Install]
WantedBy=multi-user.target
